#!/bin/bash

args=${@}

export fds_root=/fds

for i in "$@"
do
  key="$1"

  case $i in
    --fds-root=*)
    fds_root="${i#*=}"
    shift # past argument
    ;;
    *)
    # unknown option
    ;;
  esac
  shift # past argument or value
done

[ -f ${fds_root}/etc/omenv ] || exit 0
. ${fds_root}/etc/omenv

${JAVA_HOME}/bin/java ${JAVA_OPTS} -cp ${CLASSPATH} ${MAIN_CLASS} --foreground --fds-root=${fds_root} >> ${fds_root}/bin/om.out 2>&1
