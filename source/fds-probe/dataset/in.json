{
    "Run-Setup": {
        "Threads": [
            {
                "ID-Name"        : "System",
                "tp-min-thr"     : 2,
                "tp-max-thr"     : 10,
                "tp-spawn-thres" : 10,
                "tp-idle-seconds": 2
            },
            {
                "ID-Name"        : "Test",
                "tp-min-thr"     : 2,
                "tp-max-thr"     : 10,
                "tp-spawn-thres" : 10,
                "tp-idle-seconds": 2
            }
        ],
        "Server": {
            "srv-port" : 8080
        },
        "Loads": [
            {
                "ID-Name"         : "Run",
                "rt-loop"         : 1000,
                "rt-concurrent"   : 100,
                "rt-duration-sec" : 60
            },
            {
                "ID-Name"         : "Idle",
                "rt-duration-sec" : 60
            }
        ],
    },
    "Run-Input": {
        "Server-Load": [
            [ "open",  "/dev/null" ],
            [ "creat", "/tmp/" ],
            [ "time" ]
        ],
        "Client-Load": [
            {
                "ID-Name"     : "Example-script",
                "Script"      : "/bin/yes",
                "Script-Args" : "abc",
                "cache-output": true
            },
            {
                "ID-Name"     : "Sleep",
                "Script"      : "sleep",
                "Script-Args" : "10"
            }
        ],
    },
    "Run-Resources": [
        {
            "ID-Name"     : "Memory"
        },
        {
            "ID-Name"     : "CPU"
        },
        {
            "ID-Name"     : "Net-bandwidth"
        }
    ],
    "Run-Config": [
        {
            "ID-Name" : "Syscall",
            "Comp"    : [
                "System", "Run", "Server-Load", "Memory"
            ]
        },
        {
            "ID-Name" : "Ext-script",
            "Comp"    : [
                "Test", "Run", "Client-Load"
            ]
        },
        {
            "ID-Name" : "Ext-IDLE",
            "Comp"    : [
                "Idle", "Sleep"
            ]
        }
    ],
    "Run-Timeline": [
        { "Parallel": [ "Syscall", "Ext-script" ] },
        { "Barrier" : "Ext-IDLE" }, 
        { "Parallel": [ "Syscall", "Ext-script" ] }
    ]
}
