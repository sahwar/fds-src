package: fds-tools
version: 0.2
description: set of tools for fds to run

depends: 
  - fds-systemdir
  - fds-systemconf

files:
  - dest: /fds/sbin
    src:
      - ../../../Build/linux-x86_64.debug/bin/configdbtool
      - ../../redis.sh
      - ../../../test/fds-tool.py
      - ../../../test/fdsadmin
    dest-is-dir: True

  - dest: /fds/sbin
    dest-is-dir: True
    src:
      - ../../../test/formation.conf
    conf: True
